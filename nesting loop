#include<stdio.h>
#include<conio.h>

int main(){

    int n,m,roll,total =0,marks;
    printf("Enter the number of student and subject :");
    scanf("%d %d",&n,&m);
    printf("\n");
    for(int i =1; i<=n ;++i){

        printf("enter the rollnum :");
        scanf("%d",&roll);
        printf("enter the mark of %d sunjects for roll no %d \n:",m,roll);
        for(int j = 1; j <=m; j++)
        {
            scanf("%d",&marks);
            total=total+marks;
        }
        printf("total marks: %d \n" ,total);
        

    }
    
}
