#include<stdio.h>
// print all odd num in array

void countoddnum(int arr[], int n);

int main(){
    int arr[]={1,2,3,4,5,6};
    //printf("%d", countoddnum(arr,6));
    countoddnum(arr,6);
    
    return 0;
}

void countoddnum(int arr[],int n){
    int count=0;
    for (int i = 0; i < n; i++)
    {
        if (arr[i] % 2 != 0)
        {
           printf("%d\n",arr[i]);
            count++;

        }
        
    }

}
