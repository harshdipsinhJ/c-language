// sum of last 2 elements
#include<stdio.h>
#include<conio.h>
int main(){

int num[100],n,sum=0;
printf("Enter number of array elements=");
scanf("%d",&n);

for(int i=0;i<n;i++)
{
printf("Enter array element=");
scanf("%d",&num[i]);
}

sum=num[n-1]+num[n-2];
printf("the sum of last 2 elements in array =%d",sum);
return 0;
}
