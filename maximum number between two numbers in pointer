#include<stdio.h>

void maximum(int *a,int *b);

int main(){
    int a,b;
    printf("enter a=");
    scanf("%d",&a);
    printf("enter b =");
    scanf("%d",&b);
    maximum(&a,&b);
    return 0;


}
void maximum(int *a,int *b){
    if (*a>*b)
    {
        printf("max is =%d",*a);
    }
    else
    {
        printf("max is =%d",*b);
    }
    
}
